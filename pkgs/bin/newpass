#!/bin/bash
[ -z "$QUTE_URL" ] && exit
url=$(echo $QUTE_URL | sed -e "s/[^/]*\/\/\([^@]*@\)\?\([^:/]*\).*/\2/"}
username=$(dmenu -p username)
[ -z "$username" ] && exit
password=$(dmenu -password -p password)
[ -z "$password" ] && exit
passinf=$(mktemp)
cat << EOF
$password
username: $username
EOF > $passinf
echo "message-info $url/$username" > $QUTE_FIFO
pass insert -m $url/$username < $passinf



